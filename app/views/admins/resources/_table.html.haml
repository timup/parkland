%table.table.table-striped
  %thead
    %tr
      %th Name
      %th Owner
      %th Created
      %th View
  %tbody
    - resources.each do |resource|
      %tr
        %td= resource.name
        %td= resource.owner
        %td= time_ago_in_words(resource.created_at) + " ago"
        %td= link_to "View", admin_resource_path(resource)